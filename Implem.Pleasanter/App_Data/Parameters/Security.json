{
    "RequestLimit": 0,
    "PrivilegedUsers": null,
    "RevealUserDisabled": false,
    "LockoutCount": 0,
    "PasswordExpirationPeriod": 0,
    "JoeAccountCheck": true,
    "PasswordPolicies": [
        {
            "Enabled": true,
            "Regex": ".{6,}",
            "Languages": [
                {
                    "Body": "Please enter at least 6 characters for the password."
                },
                {
                    "Language": "ja",
                    "Body": "パスワードは6文字以上を入力してください。"
                }
            ]
        },
        {
            "Enabled": false,
            "Regex": "[a-z]+",
            "Languages": [
                {
                    "Body": "Please enter at least one lower case alphabetic character in the password."
                },
                {
                    "Language": "ja",
                    "Body": "パスワードにはアルファベット小文字を1文字以上を入力してください。"
                }
            ]
        },
        {
            "Enabled": false,
            "Regex": "[A-Z]+",
            "Languages": [
                {
                    "Body": "Please enter at least one alphabetic capital letter in the password."
                },
                {
                    "Language": "ja",
                    "Body": "パスワードにはアルファベット大文字を1文字以上を入力してください。"
                }
            ]
        },
        {
            "Enabled": false,
            "Regex": "[0-9]+",
            "Languages": [
                {
                    "Body": "Please enter one or more digits in the password."
                },
                {
                    "Language": "ja",
                    "Body": "パスワードには数字を1文字以上を入力してください。"
                }
            ]
        },
        {
            "Enabled": false,
            "Regex": "[^a-zA-Z0-9]+",
            "Languages": [
                {
                    "Body": "Please enter one or more symbols in the password."
                },
                {
                    "Language": "ja",
                    "Body": "パスワードには記号を1文字以上を入力してください。"
                }
            ]
        }
    ],
    "SecondaryAuthentication": {
        "Enabled": false,
        "NotificationType": "Mail",
        "AuthenticationCodeCharacterType": "Number",
        "AuthenticationCodeLength": 8,
        "AuthenticationCodeExpirationPeriod": 300
    }
}