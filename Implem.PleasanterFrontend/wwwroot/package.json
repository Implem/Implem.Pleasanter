{
    "name": "wwwroot",
    "private": true,
    "version": "0.0.0",
    "type": "commonjs",
    "scripts": {
        "dev": "npm-run-all --parallel watch:*",
        "watch:scripts": "vite build --config vite.config.dev.scripts.ts --watch --mode development",
